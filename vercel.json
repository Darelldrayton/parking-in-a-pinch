{
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://www.parkinginapinch.com",
      "permanent": true,
      "host": "pinchparking.com"
    },
    {
      "source": "/(.*)",
      "destination": "https://www.parkinginapinch.com/$1",
      "permanent": true,
      "host": "pinchparking.com"
    },
    {
      "source": "/",
      "destination": "https://www.parkinginapinch.com",
      "permanent": true,
      "host": "www.pinchparking.com"
    },
    {
      "source": "/(.*)",
      "destination": "https://www.parkinginapinch.com/$1",
      "permanent": true,
      "host": "www.pinchparking.com"
    }
  ]
}
