{
  "buildCommand": "cd frontend && npm ci && npm run build",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://165.227.111.160/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://www.parkinginapinch.com",
      "permanent": true,
      "host": "pinchparking.com"
    },
    {
      "source": "/(.*)",
      "destination": "https://www.parkinginapinch.com/$1",
      "permanent": true,
      "host": "pinchparking.com"
    }
  ]
}
