{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "framework": null,
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://165.227.111.160/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma", 
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://www.parkinginapinch.com",
      "permanent": true,
      "host": "pinchparking.com"
    },
    {
      "source": "/(.*)",
      "destination": "https://www.parkinginapinch.com/$1",
      "permanent": true,
      "host": "pinchparking.com"
    },
    {
      "source": "/",
      "destination": "https://www.parkinginapinch.com",
      "permanent": true,
      "host": "www.pinchparking.com"
    },
    {
      "source": "/(.*)",
      "destination": "https://www.parkinginapinch.com/$1",
      "permanent": true,
      "host": "www.pinchparking.com"
    }
  ],
  "cleanUrls": true
}
