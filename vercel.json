{
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/parkinginapinch.com/:path*",
      "destination": "https://parkinginapinch.com/:path*",
      "permanent": true
    },
    {
      "source": "/www.parkinginapinch.com/:path*", 
      "destination": "https://parkinginapinch.com/:path*",
      "permanent": true
    }
  ]
}